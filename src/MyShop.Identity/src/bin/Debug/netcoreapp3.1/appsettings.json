{
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341/",
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    }
  },

  "ConnectionString": "Server=.;Database=UserContext;Trusted_Connection=True;MultipleActiveResultSets=true",

  "SpaClient": "http://localhost:4200",
  "TokenLifetimeMinutes": 120,
  "PermanentTokenLifetimeDays": 365,

  "EventBusConnection": "localhost",
  "EventBusUserName": "guest",
  "EventBusPassword": "guest",
  "EventBusRetryCount": 5,

  "AzureServiceBusEnabled": false,
  "AzureStorageEnabled": false,
  "SubscriptionClientName": "myshop-identity",

  "ApplicationInsights": {
    "InstrumentationKey": ""
  },

  "UseVault": false,
  "Vault": {
    "Name": "eshop",
    "ClientId": "your-clien-id",
    "ClientSecret": "your-client-secret"
  }
}
